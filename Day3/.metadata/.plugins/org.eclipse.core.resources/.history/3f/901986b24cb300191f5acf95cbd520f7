package hcl.com.practice3;

public class Attendance {
	public static void show(String...name){
		for(String s : name){
			System.out.print(s + "");
		}
		System.out.println();
	}
	public static void main(String[] args) {
		show();
		show("Jenny","Shrey");
		show("Jenny","Shrey","Sharry");
		show("Sharry","Mona");}


}
