package com.hcl.collections;

interface IHello{
	void sayHell();
}
public class Lamda2 {
public static void main(String[] args) {
	IHello h1 = () ->{
		System.out.println("Hello from Shrey");
	};
	IHello h2 = () ->{
		System.out.println("Hello from Sharry");
	};
	IHello h3 = () ->{
		System.out.println("Hello from Jenny");
	};
	h1.sayHell();
	h2.sayHell();
	h3.sayHell();
}
}
